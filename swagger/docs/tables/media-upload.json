{
    "/tables/media-upload": {
        "tags": [
            "Tables"
        ],
        "summary": "Upload an image for a table",
        "description": "Use this API to upload an image for a specific table.",
        "operationId": "tablesMediaUpload",
        "parameters": [
            {
                "name": "id",
                "in": "query",
                "description": "Unique identifier of the table.",
                "required": true,
                "schema": {
                    "type": "integer",
                    "example": 79
                }
            }
        ],
        "requestBody": {
            "content": {
                "multipart/form-data": {
                    "schema": {
                        "type": "object",
                        "properties": {
                            "Image": {
                                "description": "The image file to upload.",
                                "type": "string",
                                "format": "binary",
                                "example": "interests-image-1.png"
                            }
                        },
                        "required": [
                            "Image"
                        ]
                    }
                }
            }
        },
        "responses": {
            "200": {
                "description": "Image uploaded successfully.",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "description": "Confirmation message.",
                                    "type": "string",
                                    "example": "Image uploaded successfully."
                                },
                                "meta": {
                                    "type": "object",
                                    "properties": {
                                        "image": {
                                            "type": "object",
                                            "properties": {
                                                "path": {
                                                    "description": "The base URL where the image is stored.",
                                                    "type": "string",
                                                    "example": "https://s3.ap-south-1.amazonaws.com/high-table"
                                                },
                                                "folder": {
                                                    "description": "The folder path where the image is stored.",
                                                    "type": "string",
                                                    "example": "public/image/interests"
                                                },
                                                "example": {
                                                    "description": "An example URL of the uploaded image.",
                                                    "type": "string",
                                                    "example": "https://s3.ap-south-1.amazonaws.com/high-table/public/image/interests/interests-image-777.jpeg"
                                                }
                                            }
                                        }
                                    }
                                },
                                "details": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "description": "Status of the table.",
                                            "type": "string",
                                            "example": "1"
                                        },
                                        "created_at": {
                                            "description": "Timestamp when the table was created.",
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2024-02-12T09:53:26.616Z"
                                        },
                                        "updated_at": {
                                            "description": "Timestamp when the table was last updated.",
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2024-08-09T11:37:08.079Z"
                                        },
                                        "id": {
                                            "description": "Unique identifier of the table.",
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "name": {
                                            "description": "Name of the table.",
                                            "type": "string",
                                            "example": "art"
                                        },
                                        "image": {
                                            "description": "File name of the uploaded image.",
                                            "type": "string",
                                            "example": "interests-image-1.png"
                                        },
                                        "width": {
                                            "description": "Width of the image.",
                                            "type": "integer",
                                            "example": 11
                                        },
                                        "height": {
                                            "description": "Height of the image.",
                                            "type": "integer",
                                            "example": 11
                                        }
                                    }
                                }
                            },
                            "required": [
                                "message",
                                "meta",
                                "details"
                            ]
                        }
                    }
                }
            },
            "400": {
                "description": "Bad request"
            },
            "401": {
                "description": "Unauthorized"
            },
            "403": {
                "description": "You don't have permission to perform this action"
            },
            "500": {
                "description": "Internal server error"
            }
        }
    }
}
