{
    "/tables/list-admin": {
        "tags": [
            "Admin"
        ],
        "summary": "Lists all tables to admin",
        "description": "Use this API to show all tables.",
        "operationId": "tablesListAdmin",
        "requestBody": {
            "content": {
                "application/json": {
                    "schema": {
                        "type": "object",
                        "properties": {
                            "page": {
                                "description": "Page number to retrieve.",
                                "type": "string",
                                "example": "1"
                            },
                            "limit": {
                                "description": "Number of items per page.",
                                "type": "string",
                                "example": "1"
                            }
                        }
                    }
                }
            }
        },
        "responses": {
            "200": {
                "description": "Tables retrieved successfully.",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "description": "Confirmation message.",
                                    "type": "string",
                                    "example": "Tables retrieved successfully."
                                },
                                "meta": {
                                    "type": "object",
                                    "properties": {
                                        "page": {
                                            "description": "Current page number.",
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "limit": {
                                            "description": "Number of items per page.",
                                            "type": "integer",
                                            "example": 10
                                        },
                                        "total": {
                                            "description": "Total number of items.",
                                            "type": "integer",
                                            "example": 11
                                        }
                                    }
                                },
                                "items": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "description": "Unique identifier of the table.",
                                                "type": "integer",
                                                "example": 956
                                            },
                                            "full_name": {
                                                "description": "Full name of the user associated with the table.",
                                                "type": "string",
                                                "example": "Deepak"
                                            },
                                            "type": {
                                                "description": "Type of table (1 for Standard, 2 for Premium).",
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "media": {
                                                "description": "Media associated with the event.",
                                                "type": "string",
                                                "example": "https://s3.ap-so1137uth-1.amazonaws.com/high-table/public/photo/tables/tables-media-956.jpg"
                                            },
                                            "video": {
                                                "description": "Video associated with the event, if any.",
                                                "type": "string",
                                                "example": null
                                            },
                                            "description": {
                                                "description": "Description of the event.",
                                                "type": "string",
                                                "example": "I am hosting a curated table exclusively for Indian startup founders"
                                            },
                                            "phone": {
                                                "description": "Phone number associated with the event.",
                                                "type": "string",
                                                "example": ""
                                            },
                                            "min_seats": {
                                                "description": "Minimum number of seats available.",
                                                "type": "integer",
                                                "example": 4
                                            },
                                            "max_seats": {
                                                "description": "Maximum number of seats available.",
                                                "type": "integer",
                                                "example": 10
                                            },
                                            "booked_seats": {
                                                "description": "Number of seats booked.",
                                                "type": "integer",
                                                "example": 0
                                            },
                                            "booked": {
                                                "description": "Indicates if the table is booked (0 for no, 1 for yes).",
                                                "type": "integer",
                                                "example": 0
                                            },
                                            "bookmarks": {
                                                "description": "Number of bookmarks.",
                                                "type": "integer",
                                                "example": 0
                                            },
                                            "followers": {
                                                "description": "Number of followers.",
                                                "type": "integer",
                                                "example": 0
                                            },
                                            "title": {
                                                "description": "Title of the event.",
                                                "type": "string",
                                                "example": "Founders Table"
                                            },
                                            "price": {
                                                "description": "Price of the event.",
                                                "type": "integer",
                                                "example": 35
                                            },
                                            "tags": {
                                                "description": "Tags associated with the event.",
                                                "type": "string",
                                                "example": "Business"
                                            },
                                            "address": {
                                                "description": "Address of the event.",
                                                "type": "string",
                                                "example": "Third Wave Coffee, Ganapathi Temple Road, Koramangala 8th Block, Koramangala, Bengaluru, Karnataka, India"
                                            },
                                            "city": {
                                                "description": "City where the event is located.",
                                                "type": "string",
                                                "example": ""
                                            },
                                            "created_by": {
                                                "description": "ID of the user who created the event.",
                                                "type": "integer",
                                                "example": 785
                                            },
                                            "event_date": {
                                                "description": "Date and time of the event.",
                                                "type": "string",
                                                "example": "09-09-2024 10:30"
                                            },
                                            "reviews": {
                                                "description": "Number of reviews for the event.",
                                                "type": "integer",
                                                "example": 0
                                            },
                                            "status": {
                                                "description": "Status of the event.",
                                                "type": "integer",
                                                "example": 3
                                            },
                                            "admin_id": {
                                                "description": "ID of the admin associated with the event.",
                                                "type": "integer",
                                                "example": 0
                                            },
                                            "user_type": {
                                                "description": "Type of user (1 for normal user, 2 for admin).",
                                                "type": "integer",
                                                "example": 2
                                            },
                                            "location": {
                                                "type": "object",
                                                "properties": {
                                                    "lat": {
                                                        "description": "Latitude of the event location.",
                                                        "type": "number",
                                                        "example": 12.9396226
                                                    },
                                                    "lng": {
                                                        "description": "Longitude of the event location.",
                                                        "type": "number",
                                                        "example": 77.6194182
                                                    }
                                                }
                                            },
                                            "event_done_flag": {
                                                "description": "Flag indicating if the event is done.",
                                                "type": "boolean",
                                                "example": false
                                            },
                                            "category": {
                                                "description": "Category ID of the event.",
                                                "type": "integer",
                                                "example": 4
                                            },
                                            "booked_status": {
                                                "description": "Indicates if the table is booked (true for booked, false for not booked).",
                                                "type": "boolean",
                                                "example": false
                                            },
                                            "user_details": {
                                                "type": "object",
                                                "properties": {
                                                    "photo": {
                                                        "description": "Photo URL of the user who created the event.",
                                                        "type": "string",
                                                        "example": "https://s3.ap-south-1.amazonaws.com/high-table/public/photo/members/"
                                                    }
                                                }
                                            },
                                            "category_name": {
                                                "description": "Name of the category.",
                                                "type": "string",
                                                "example": "startups"
                                            },
                                            "location_details": {
                                                "type": "object",
                                                "properties": {
                                                    "state": {
                                                        "description": "State where the event is located.",
                                                        "type": "string",
                                                        "example": "Karnataka"
                                                    },
                                                    "city": {
                                                        "description": "City where the event is located.",
                                                        "type": "string",
                                                        "example": "Bengaluru"
                                                    },
                                                    "pincode": {
                                                        "description": "Pincode of the event location.",
                                                        "type": "string",
                                                        "example": "560001"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "400": {
                "description": "Bad request"
            },
            "401": {
                "description": "Unauthorized"
            },
            "403": {
                "description": "You don't have permission to perform this action"
            },
            "500": {
                "description": "Internal server error"
            }
        }
    }
}