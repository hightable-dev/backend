{
  "/tables/view": {
    "tags": [
      "Tables"
    ],
    "summary": "View Table",
    "description": "Use this API to view the table details.",
    "operationId": "/tables/view",
    "parameters": [
      {
        "name": "/tables/view?",
        "in": "query",
        "description": "Table ID",
        "required": true,
        "schema": {
          "type": "string",
          "example": "id=955"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "Tables retrieved successfully.",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "description": "Response message.",
                  "type": "string",
                  "example": "Tables retrieved successfully."
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "description": "Unique identifier of the table.",
                      "type": "integer",
                      "example": 955
                    },
                    "full_name": {
                      "description": "Full name of the table owner.",
                      "type": "string",
                      "example": "Sathish"
                    },
                    "type": {
                      "description": "Type of the table.",
                      "type": "integer",
                      "example": 1
                    },
                    "media": {
                      "description": "URL of the table's media.",
                      "type": "string",
                      "example": "https://s3.ap-south-1.amazonaws.com/high-table/public/photo/tables/tables-media-955.png"
                    },
                    "video": {
                      "description": "URL of the table's video.",
                      "type": "string",
                      "nullable": true,
                      "example": null
                    },
                    "description": {
                      "description": "Description of the table.",
                      "type": "string",
                      "example": "The best way to do work is to love what you do."
                    },
                    "phone": {
                      "description": "Contact phone number.",
                      "type": "string",
                      "example": ""
                    },
                    "min_seats": {
                      "description": "Minimum number of seats available.",
                      "type": "integer",
                      "example": 4
                    },
                    "max_seats": {
                      "description": "Maximum number of seats available.",
                      "type": "integer",
                      "example": 10
                    },
                    "booked_seats": {
                      "description": "Number of seats currently booked.",
                      "type": "integer",
                      "example": 0
                    },
                    "booked": {
                      "description": "Number of bookings made.",
                      "type": "integer",
                      "example": 0
                    },
                    "bookmarks": {
                      "description": "Number of times the table has been bookmarked.",
                      "type": "integer",
                      "example": 0
                    },
                    "followers": {
                      "description": "Number of followers.",
                      "type": "integer",
                      "example": 0
                    },
                    "title": {
                      "description": "Title of the table.",
                      "type": "string",
                      "example": "Natural"
                    },
                    "price": {
                      "description": "Price of the table.",
                      "type": "integer",
                      "example": 35
                    },
                    "tags": {
                      "description": "Tags associated with the table.",
                      "type": "string",
                      "example": "Celebrity,Festival"
                    },
                    "address": {
                      "description": "Address of the table.",
                      "type": "string",
                      "example": "Chennai, Tamil Nadu, India"
                    },
                    "city": {
                      "description": "City where the table is located.",
                      "type": "string",
                      "example": ""
                    },
                    "created_by": {
                      "description": "ID of the user who created the table.",
                      "type": "integer",
                      "example": 740
                    },
                    "event_date": {
                      "description": "Event date of the table.",
                      "type": "string",
                      "example": "28-08-2024 15:47"
                    },
                    "reviews": {
                      "description": "Number of reviews for the table.",
                      "type": "integer",
                      "example": 0
                    },
                    "status": {
                      "description": "Current status of the table.",
                      "type": "integer",
                      "example": 3
                    },
                    "admin_id": {
                      "description": "ID of the admin managing the table.",
                      "type": "integer",
                      "example": 0
                    },
                    "user_type": {
                      "description": "Type of user (creator, follower, etc.).",
                      "type": "integer",
                      "example": 2
                    },
                    "location": {
                      "description": "Geographical location of the table.",
                      "type": "object",
                      "properties": {
                        "lat": {
                          "description": "Latitude of the location.",
                          "type": "number",
                          "example": 13.0843007
                        },
                        "lng": {
                          "description": "Longitude of the location.",
                          "type": "number",
                          "example": 80.2704622
                        }
                      }
                    },
                    "event_done_flag": {
                      "description": "Flag indicating if the event is completed.",
                      "type": "boolean",
                      "example": false
                    },
                    "category_name": {
                      "description": "Category name of the table.",
                      "type": "string",
                      "example": "general"
                    },
                    "location_details": {
                      "description": "Detailed location information.",
                      "type": "object",
                      "properties": {
                        "state": {
                          "description": "State where the table is located.",
                          "type": "string",
                          "example": "Tamil Nadu"
                        },
                        "city": {
                          "description": "City where the table is located.",
                          "type": "string",
                          "example": "Chennai"
                        },
                        "pincode": {
                          "description": "Pincode of the table's location.",
                          "type": "string",
                          "example": "600003"
                        }
                      }
                    },
                    "user_details": {
                      "description": "Details of the user who created the table.",
                      "type": "object",
                      "properties": {
                        "photo": {
                          "description": "URL of the user's photo.",
                          "type": "string",
                          "example": "https://s3.ap-south-1.amazonaws.com/high-table/public/photo/members/members-photo-740.png"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "400": {
          "description": "Bad request"
        },
        "401": {
          "description": "Unauthorized"
        },
        "403": {
          "description": "You don't have permission to perform this action"
        },
        "500": {
          "description": "Internal server error"
        }
      }
    }
  }
}
