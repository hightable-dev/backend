{
    "/tables/table-search": {
        "tags": [
            "Tables"
        ],
        "summary": "Search for tables",
        "description": "Use this API to search for tables based on various criteria. If matches are found, it returns the count and list of tables. If no matches are found, it lists all tables.",
        "operationId": "tableSearch",
        "requestBody": {
            "content": {
                "application/json": {
                    "schema": {
                        "type": "object",
                        "properties": {
                            "address": {
                                "description": "Address to filter the tables.",
                                "type": "string",
                                "example": "Karnataka"
                            },
                            "search": {
                                "description": "Search keyword for filtering tables.",
                                "type": "string",
                                "example": ""
                            },
                            "from": {
                                "description": "Start date for the search range.",
                                "type": "string",
                                "format": "date",
                                "example": "09-09-2024"
                            },
                            "to": {
                                "description": "End date for the search range.",
                                "type": "string",
                                "format": "date",
                                "example": "09-09-2024"
                            }
                        }
                    }
                }
            }
        },
        "responses": {
            "200": {
                "description": "Tables retrieved successfully.",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "description": "Confirmation message.",
                                    "type": "string",
                                    "example": "Tables retrieved successfully."
                                },
                                "meta": {
                                    "type": "object",
                                    "properties": {
                                        "page": {
                                            "description": "Current page number.",
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "limit": {
                                            "description": "Number of items per page.",
                                            "type": "integer",
                                            "example": 10
                                        },
                                        "total": {
                                            "description": "Total number of tables matching the search criteria.",
                                            "type": "integer",
                                            "example": 1
                                        }
                                    },
                                    "required": [
                                        "page",
                                        "limit",
                                        "total"
                                    ]
                                },
                                "isRecommanded": {
                                    "description": "Indicates if the table is recommended.",
                                    "type": "boolean",
                                    "example": false
                                },
                                "items": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "description": "Unique identifier of the table.",
                                                "type": "integer",
                                                "example": 956
                                            },
                                            "full_name": {
                                                "description": "Full name associated with the table.",
                                                "type": "string",
                                                "example": "Deepak"
                                            },
                                            "type": {
                                                "description": "Type of the table.",
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "media": {
                                                "description": "URL of the media associated with the table.",
                                                "type": "string",
                                                "example": "https://s3.ap-south-1.amazonaws.com/high-table/public/photo/tables/tables-media-956.jpg"
                                            },
                                            "video": {
                                                "description": "URL of the video associated with the table.",
                                                "type": "string",
                                                "example": "null"
                                            },
                                            "description": {
                                                "description": "Description of the table.",
                                                "type": "string",
                                                "example": "I am hosting a curated table exclusively for Indian startup founders"
                                            },
                                            "phone": {
                                                "description": "Phone number associated with the table.",
                                                "type": "string",
                                                "example": ""
                                            },
                                            "min_seats": {
                                                "description": "Minimum number of seats available.",
                                                "type": "integer",
                                                "example": 4
                                            },
                                            "max_seats": {
                                                "description": "Maximum number of seats available.",
                                                "type": "integer",
                                                "example": 10
                                            },
                                            "booked_seats": {
                                                "description": "Number of seats booked.",
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "booked": {
                                                "description": "Booking status.",
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "bookmarks": {
                                                "description": "Number of bookmarks.",
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "followers": {
                                                "description": "Number of followers.",
                                                "type": "integer",
                                                "example": 0
                                            },
                                            "title": {
                                                "description": "Title of the table.",
                                                "type": "string",
                                                "example": "Founders Table"
                                            },
                                            "price": {
                                                "description": "Price of the table.",
                                                "type": "integer",
                                                "example": 35
                                            },
                                            "tags": {
                                                "description": "Tags associated with the table.",
                                                "type": "string",
                                                "example": "Business"
                                            },
                                            "address": {
                                                "description": "Address of the table.",
                                                "type": "string",
                                                "example": "Third Wave Coffee, Ganapathi Temple Road, Koramangala 8th Block, Koramangala, Bengaluru, Karnataka, India"
                                            },
                                            "city": {
                                                "description": "City of the table.",
                                                "type": "string",
                                                "example": ""
                                            },
                                            "created_by": {
                                                "description": "ID of the user who created the table.",
                                                "type": "integer",
                                                "example": 785
                                            },
                                            "event_date": {
                                                "description": "Event date related to the table.",
                                                "type": "string",
                                                "example": "09-09-2024 10:30"
                                            },
                                            "reviews": {
                                                "description": "Number of reviews.",
                                                "type": "integer",
                                                "example": 0
                                            },
                                            "status": {
                                                "description": "Status of the table.",
                                                "type": "integer",
                                                "example": 3
                                            },
                                            "admin_id": {
                                                "description": "ID of the admin.",
                                                "type": "integer",
                                                "example": 0
                                            },
                                            "user_type": {
                                                "description": "Type of user associated with the table.",
                                                "type": "integer",
                                                "example": 2
                                            },
                                            "location": {
                                                "type": "object",
                                                "properties": {
                                                    "lat": {
                                                        "description": "Latitude of the table's location.",
                                                        "type": "number",
                                                        "format": "float",
                                                        "example": 12.9396226
                                                    },
                                                    "lng": {
                                                        "description": "Longitude of the table's location.",
                                                        "type": "number",
                                                        "format": "float",
                                                        "example": 77.6194182
                                                    }
                                                },
                                                "required": [
                                                    "lat",
                                                    "lng"
                                                ]
                                            },
                                            "event_done_flag": {
                                                "description": "Event completion flag.",
                                                "type": "boolean",
                                                "example": false
                                            },
                                            "category": {
                                                "description": "Category of the table.",
                                                "type": "integer",
                                                "example": 4
                                            },
                                            "user_details": {
                                                "type": "object",
                                                "properties": {
                                                    "photo": {
                                                        "description": "URL of the user's photo.",
                                                        "type": "string",
                                                        "example": "https://s3.ap-south-1.amazonaws.com/high-table/public/photo/members/"
                                                    }
                                                }
                                            },
                                            "category_name": {
                                                "description": "Name of the category.",
                                                "type": "string",
                                                "example": "startups"
                                            }
                                        },
                                        "required": [
                                            "id",
                                            "full_name",
                                            "type",
                                            "media",
                                            "video",
                                            "description",
                                            "min_seats",
                                            "max_seats",
                                            "booked_seats",
                                            "booked",
                                            "bookmarks",
                                            "followers",
                                            "title",
                                            "price",
                                            "tags",
                                            "address",
                                            "created_by",
                                            "event_date",
                                            "reviews",
                                            "status",
                                            "user_type",
                                            "location",
                                            "event_done_flag",
                                            "category",
                                            "user_details",
                                            "category_name"
                                        ]
                                    }
                                }
                            },
                            "required": [
                                "message",
                                "meta",
                                "isRecommanded",
                                "items"
                            ]
                        }
                    }
                }
            },
            "400": {
                "description": "Bad request"
            },
            "401": {
                "description": "Unauthorized"
            },
            "403": {
                "description": "You don't have permission to perform this action"
            },
            "500": {
                "description": "Internal server error"
            }
        }
    }
}
