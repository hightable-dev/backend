{
    "/tables/update": {
        "tags": [
            "Tables"
        ],
        "summary": "Modify the table",
        "description": "Update the table details by the host.",
        "operationId": "tablesUpdate",
        "requestBody": {
            "required": true,
            "content": {
                "application/json": {
                    "schema": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "integer",
                                "description": "ID of the table to update",
                                "example": 4654
                            },
                            "type": {
                                "type": "integer",
                                "description": "Type of the table (e.g., 1 for standard, 2 for premium)",
                                "example": 2
                            },
                            "address": {
                                "type": "string",
                                "description": "Address where the table is located",
                                "example": "a"
                            },
                            "event_date": {
                                "type": "string",
                                "description": "Event date and time",
                                "example": "13-12-2024 06:00"
                            },
                            "media": {
                                "type": "string",
                                "nullable": true,
                                "description": "Media associated with the table",
                                "example": "photos.jpg"
                            },
                            "phone": {
                                "type": "string",
                                "description": "Contact phone number",
                                "example": "123"
                            },
                            "min_seats": {
                                "type": "integer",
                                "description": "Minimum number of seats at the table",
                                "example": 10
                            },
                            "max_seats": {
                                "type": "integer",
                                "description": "Maximum number of seats at the table",
                                "example": 100
                            },
                            "title": {
                                "type": "string",
                                "description": "Title of the table",
                                "example": "test"
                            },
                            "city": {
                                "type": "string",
                                "description": "City where the table is located",
                                "example": "x"
                            },
                            "description": {
                                "type": "string",
                                "description": "Description of the table",
                                "example": "xx"
                            },
                            "category": {
                                "type": "integer",
                                "description": "Category of the table",
                                "example": 4
                            },
                            "price": {
                                "type": "integer",
                                "description": "Price of the table",
                                "example": 200
                            },
                            "tags": {
                                "type": "string",
                                "description": "Comma-separated tags associated with the table",
                                "example": "1"
                            },
                            "location": {
                                "type": "object",
                                "properties": {
                                    "lat": {
                                        "type": "number",
                                        "format": "float",
                                        "description": "Latitude of the table location",
                                        "example": 1
                                    },
                                    "lng": {
                                        "type": "number",
                                        "format": "float",
                                        "description": "Longitude of the table location",
                                        "example": 1
                                    }
                                },
                                "required": [
                                    "lat",
                                    "lng"
                                ]
                            }
                        },
                        "required": [
                            "id",
                            "type",
                            "address",
                            "event_date",
                            "media",
                            "phone",
                            "min_seats",
                            "max_seats",
                            "title",
                            "city",
                            "description",
                            "category",
                            "price",
                            "tags",
                            "location"
                        ]
                    }
                }
            }
        },
        "responses": {
            "200": {
                "description": "Table data updated successfully",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "example": "Table data updated successfully"
                                },
                                "details": {
                                    "type": "object",
                                    "properties": {
                                        "thumbnail": {
                                            "type": "string",
                                            "nullable": true,
                                            "example": null
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2024-08-09T13:33:53.055Z"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2024-08-10T08:33:20.886Z"
                                        },
                                        "id": {
                                            "type": "integer",
                                            "example": 965
                                        },
                                        "full_name": {
                                            "type": "string",
                                            "example": "Demo account"
                                        },
                                        "type": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "media": {
                                            "type": "string",
                                            "nullable": true,
                                            "example": null
                                        },
                                        "video": {
                                            "type": "string",
                                            "example": ""
                                        },
                                        "description": {
                                            "type": "string",
                                            "example": "This table created by admin"
                                        },
                                        "phone": {
                                            "type": "string",
                                            "example": ""
                                        },
                                        "min_seats": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "max_seats": {
                                            "type": "integer",
                                            "example": 10
                                        },
                                        "booked_seats": {
                                            "type": "integer",
                                            "example": 0
                                        },
                                        "booked": {
                                            "type": "integer",
                                            "example": 0
                                        },
                                        "bookmarks": {
                                            "type": "integer",
                                            "example": 0
                                        },
                                        "followers": {
                                            "type": "integer",
                                            "example": 0
                                        },
                                        "title": {
                                            "type": "string",
                                            "example": "Manager created testing"
                                        },
                                        "price": {
                                            "type": "integer",
                                            "example": 35
                                        },
                                        "tags": {
                                            "type": "string",
                                            "example": "wooden, modern"
                                        },
                                        "address": {
                                            "type": "string",
                                            "example": "Banagalore, Whitefield"
                                        },
                                        "city": {
                                            "type": "string",
                                            "example": ""
                                        },
                                        "created_by": {
                                            "type": "integer",
                                            "example": 731
                                        },
                                        "event_date": {
                                            "type": "string",
                                            "example": "13-12-2024 06:00"
                                        },
                                        "reviews": {
                                            "type": "integer",
                                            "example": 3
                                        },
                                        "status": {
                                            "type": "integer",
                                            "example": 2
                                        },
                                        "admin_id": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "user_type": {
                                            "type": "integer",
                                            "example": 0
                                        },
                                        "location": {
                                            "type": "object",
                                            "properties": {
                                                "latitude": {
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": 40.7128
                                                },
                                                "longitude": {
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": -74.006
                                                }
                                            },
                                            "required": [
                                                "latitude",
                                                "longitude"
                                            ]
                                        },
                                        "event_done_flag": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "category": {
                                            "type": "integer",
                                            "example": 4
                                        }
                                    },
                                    "required": [
                                        "id",
                                        "full_name",
                                        "type",
                                        "description",
                                        "phone",
                                        "min_seats",
                                        "max_seats",
                                        "booked_seats",
                                        "booked",
                                        "bookmarks",
                                        "followers",
                                        "title",
                                        "price",
                                        "tags",
                                        "address",
                                        "city",
                                        "created_by",
                                        "event_date",
                                        "reviews",
                                        "status",
                                        "admin_id",
                                        "user_type",
                                        "location",
                                        "event_done_flag",
                                        "category"
                                    ]
                                }
                            },
                            "required": [
                                "message",
                                "details"
                            ]
                        }
                    }
                }
            },
            "400": {
                "description": "Table not found",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string",
                                    "example": "Table not found"
                                }
                            },
                            "required": [
                                "error"
                            ]
                        }
                    }
                }
            },
            "403": {
                "description": "Forbidden"
            },
            "500": {
                "description": "Internal server error"
            }
        }
    }
}
