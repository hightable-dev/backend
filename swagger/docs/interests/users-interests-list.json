{
    "/interests/users-interests-list": {
        "tags": [
            "Interests"
        ],
        "summary": "Lists all tables",
        "description": "Use this API to list all tables and ensure the event has not expired.",
        "operationId": "interestUserInterestsList",
        "requestBody": {
            "content": {
                "application/json": {
                    "schema": {
                        "type": "object",
                        "properties": {
                            "page": {
                                "description": "Page number to retrieve.",
