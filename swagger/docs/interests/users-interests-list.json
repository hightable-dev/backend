                                                "height": {
                                                    "description": "Height of the interest image in pixels.",
                                                    "type": "integer",
                                                    "example": 11
                                                }
                                            },
                                            "required": [
                                                "id",
                                                "name",
                                                "image",
                                                "width",
                                                "height"
                                            ]
                                        }
                                    }
                                },
                                "required": [
                                    "message",
                                    "meta",
                                    "items"
                                ]
                            }
